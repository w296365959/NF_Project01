<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/quoteLayout"
    style="@style/null_bg_style"
    android:layout_height="64dp">
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/default_text_color_100"
        android:textSize="@dimen/font_size_24"
        android:text="@string/value_null"/>
    <TextView
        android:id="@+id/text12"
        android:layout_marginLeft="116dp"
        app:layout_constraintLeft_toLeftOf="@id/text11"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/high"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="@id/text12"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_value_text_style"/>
    <TextView
        android:id="@+id/text14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="84dp"
        app:layout_constraintLeft_toLeftOf="@id/text13"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/today_open"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toLeftOf="@id/text14"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_value_text_style"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        app:layout_constraintLeft_toLeftOf="@id/text11"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/default_text_color_100"
        android:textSize="@dimen/font_size_14"
        android:text="@string/value_null_2"/>
    <TextView
        android:id="@+id/text22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text12"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/low"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text13"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_value_text_style"/>
    <TextView
        android:id="@+id/text24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text14"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/yestoday_close"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text15"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_value_text_style"/>
    <ImageView
        android:id="@+id/arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/detail_quote_arrow"
        android:layout_marginRight="6dp"/>
</android.support.constraint.ConstraintLayout>