<com.sscf.investment.detail.view.LandscapeDetailQuoteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/quoteLayout"
    android:layout_width="match_parent"
    android:layout_height="48dp">
    <ImageView
        android:id="@+id/close_button"
        android:layout_width="48dp"
        android:layout_height="0dp"
        android:paddingLeft="14dp"
        android:paddingRight="14dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/close"/>
    <ImageView
        android:id="@+id/to_left_button"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="14dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/to_left"/>
    <ImageView
        android:id="@+id/to_right_button"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="120dp"
        app:layout_constraintLeft_toRightOf="@id/to_left_button"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/to_right"/>
    <TextView
        android:id="@+id/name"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintLeft_toRightOf="@id/to_left_button"
        app:layout_constraintRight_toLeftOf="@id/to_right_button"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal"
        android:maxLines="1"
        android:ellipsize="end"
        android:textColor="@color/default_text_color_100"
        android:textSize="@dimen/font_size_16"/>
    <TextView
        android:id="@+id/code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/to_left_button"
        app:layout_constraintRight_toLeftOf="@id/to_right_button"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="4dp"
        android:textColor="@color/default_text_color_60"
        android:textSize="@dimen/font_size_14"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="6dp"
        app:layout_constraintLeft_toRightOf="@id/to_right_button"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/default_text_color_100"
        android:textSize="@dimen/font_size_22"
        android:text="@string/value_null"/>
    <TextView
        android:id="@+id/text12"
        android:layout_marginLeft="100dp"
        app:layout_constraintLeft_toLeftOf="@id/text11"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/high"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        app:layout_constraintLeft_toRightOf="@id/text12"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_value_text_style"/>
    <TextView
        android:id="@+id/text14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        app:layout_constraintLeft_toRightOf="@id/text12"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/open"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        app:layout_constraintLeft_toRightOf="@id/text14"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_value_text_style"/>
    <TextView
        android:id="@+id/text16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="80dp"
        app:layout_constraintLeft_toRightOf="@id/text14"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/turnover"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        app:layout_constraintLeft_toRightOf="@id/text16"
        app:layout_constraintTop_toTopOf="@id/text11"
        style="@style/security_detail_quote_value_text_style"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        app:layout_constraintLeft_toLeftOf="@id/text11"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/default_text_color_100"
        android:textSize="@dimen/font_size_12"
        android:text="@string/value_null_2"/>
    <TextView
        android:id="@+id/text22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text12"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/low"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text13"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_value_text_style"/>
    <TextView
        android:id="@+id/text24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text14"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/amount_short"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text15"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_value_text_style"/>
    <TextView
        android:id="@+id/text26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text16"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_key_text_style"
        android:text="@string/pe"/>
    <com.sscf.investment.component.ui.widget.NumberTextView
        android:id="@+id/text27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/text17"
        app:layout_constraintBottom_toBottomOf="@id/text21"
        style="@style/security_detail_quote_value_text_style"/>
</com.sscf.investment.detail.view.LandscapeDetailQuoteLayout>